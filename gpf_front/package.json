{
  "name": "argon-dashboard-react",
  "version": "1.2.2",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Creative Tim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
  },
  "scripts": {
    "start": "react-scripts start --public-url=/index",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "axios": "^1.3.4",
    "bootstrap": "4.6.0",
    "bulma": "^0.9.4",
    "chart.js": "^4.2.1",
    "classnames": "2.3.1",
    "d3-3d": "^0.1.2",
    "moment": "2.29.4",
    "nouislider": "15.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-card-flip": "^1.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "^3.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.2",
    "react-toastify": "^9.1.2",
    "reactstrap": "^8.9.0",
    "recharts": "^2.7.2",
    "sass": "1.54.5",
    "semantic-ui-react": "^2.1.4",
    "sweetalert2": "^11.7.3",
    "sweetalert2-react-content": "^5.0.7"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "jquery": "3.6.0",
    "typescript": "4.8.2"
  }
}
